 $(document).ready(function(){
    var images = ['dog-cat-1.jpg','dog-cat-10.jpg','dog-cat-2.jpg','dog-cat-3.jpg','dog-cat-4.jpg','dog-cat-5.jpg','dog-cat-6.jpg','dog-cat-7.jpg','dog-cat-8.jpg','dog-cat-9.jpg','dot-cat-2.jpg'];

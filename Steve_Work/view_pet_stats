 <script type="text/javascript">     
 $(document).ready( function() {
      var search_query;
	  function build_search_results (response) {
        var search_results_html;
        var shown_results_count = response.form_data.results.length;

		if (shown_results_count > 0) {
		    var search_results_html = "<p class='body-bold'>We found the following topics that might be helpful</p><ul>";
		    var count;
		    for ( count = 0; count < shown_results_count; count++ ) {
		    		var title = response.form_data.results[count].title;
		    		var html_url   = response.form_data.results[count].html_url;
		    	
		    		search_results_html += '<li><a href="' + html_url + '"  target="_blank" >' + title + '</a></li>';

		    }

            search_results_html += "</ul>";
            var total_results_count = response.form_data.count;
            if ( total_results_count > shown_results_count ) {
                search_results_html += '<p class="body-bold seeall"><a href="https://adoptapetcom.zendesk.com/hc/en-us/search?query='+search_query+'" target="_blank">See all '+total_results_count+' results</a></p>';
            }
		}
		else 
		{
			search_results_html = "<p class='body-bold'>There are no topics that match you search.  Please search again.</p>";
		}
	  

		$("div#target_div").html(search_results_html);
	  }

	  function call_zendesk_api(search_query){
		$('#throbber').toggle();  
	  	$.ajax({
		      type: "GET",
		  	  url: "/zendesk/public_search",
			  cache: false,
			  dataType: "json",
			  data: { query: search_query },
			  success: function(response){
				  build_search_results(response);
				  $('#throbber').toggle();
				  },
              error: function(response){
                    //TODO
                  $("div#target_div").html('<p class="body-bold">No results found - please try again</p>');
				  $('#throbber').toggle();
              }
	  	});
      }

	  $("#searchbutton").click(function(event){
		event.preventDefault();
		search_query = $("#zendeskinput").val();
        if (search_query.length == 0) {
              $("div#target_div").html('<p class="body-bold">No search terms entered - please try again</p>');
        }
        else {
	        call_zendesk_api(search_query);
        }
		})
	 
  });
 
 </script>
<script type="text/javascript">  
  $(function() {
	 var shelter_contact = $( "#shelter_contact" );
		shelter_contact.dialog({
			modal: true,
            autoOpen: false,
			resizable: false,
			draggable: false,
			width: 658,
			height: 550,
			dialogClass: "shelter_contact_dialog",
			buttons: {
                    Close: function() {
                        $( this ).dialog( "close" );
                        }
            }
		
       });
 
        $('#contact_us').click(function() {
            shelter_contact.dialog( "open" );
        });
}); 
</script>

